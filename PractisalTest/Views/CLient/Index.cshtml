@model List<ClientGetDto>
<!DOCTYPE html>
<html>
<head>
    <title>Page Title</title>
</head>
<body>

    <table class="table">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">FirstName</th>
                <th scope="col">LastName</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <th scope="row">@item.ClientUniqueId</th>
                    <td>@item.Name</td>
                    <td>@item.Surname</td>
                </tr>
            }

        </tbody>
    </table>

</body>
</html>